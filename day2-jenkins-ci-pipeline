pipeline {
    agent any
    stages {
        stage('code from scm') {
            steps {
                echo "geeting code from scm"
            }
        }
        stage('mvn build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('mvn test') {
            steps {
                sh 'mvn test'
                junit 'target/surefire-reports/*.xml'
            }
        }
    }
    
}
